{
  "name": "css-shortener",
  "description": "Utility to shorten css class names",
  "version": "3.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release",
    "typedoc": "typedoc --out docs src/index.ts --excludePrivate"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbrandau/css-shortener.git"
  },
  "keywords": [
    "css",
    "short",
    "class",
    "name",
    "shorten"
  ],
  "author": "Maximilian Brandau",
  "bugs": {
    "url": "https://github.com/mbrandau/css-shortener/issues"
  },
  "homepage": "https://github.com/mbrandau/css-shortener#readme",
  "module": "dist/tsdx.esm.js",
  "size-limit": [
    {
      "path": "dist/css-shortener.cjs.production.min.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/css-shortener.esm.js",
      "limit": "1 KB"
    }
  ],
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@size-limit/preset-small-lib": "7.0.8",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "husky": "7.0.4",
    "semantic-release": "19.0.2",
    "size-limit": "7.0.8",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typedoc": "0.22.15",
    "typescript": "4.6.3"
  }
}
